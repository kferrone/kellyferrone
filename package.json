{
  "name": "kellyferrone-admin",
  "version": "0.0.5",
  "description": "An embeddable Google Apps Script admin interface for the blog",
  "main": "index.js",
  "scripts": {
    "gas": "webpack --config ./server/webpack.gas.js ",
    "build": "webpack --config ./client/webpack.dev.js ",
    "local": "webpack-dev-server --config ./client/webpack.dev.js --open",
    "prod": "webpack --config ./client/webpack.prod.js && rm dist/gas/app.bundle.js",
    "deploy": "npm run prod && npm run gas && clasp push -f && clasp open --webapp"
  },
  "keywords": [
    "clasp",
    "gas",
    "apps script",
    "google drive",
    "gcp",
    "vue",
    "vuejs"
  ],
  "author": "Kelly Ferrone",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "popper.js": "^1.14.7",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "purecss": "^1.0.1",
    "quill": "^1.3.7",
    "raw-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0"
  }
}
